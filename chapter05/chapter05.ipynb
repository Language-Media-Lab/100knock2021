      "source": [
        "#文節を表すクラスChunk\n",
        "class Chunk():\n",
        "  def __init__(self,morphs,dst):#constructor\n",
        "    self.morphs = morphs\n",
        "    self.dst = dst\n",
        "    self.srcs = []"
      ],
      "execution_count": 8,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
