      "source": [
        "#形態素を表すクラスMorph\n",
        "class Morph:\n",
        "  def __init__(self,Morph):#constructor\n",
        "    list = Morph.split()\n",
        "    if list!=[] and len(list)!=1:#改行文字を除外する\n",
        "      morpheme_info = list[1].split(',')\n",
        "      if morpheme_info[0]!='': #['', '']を除外する\n",
        "          self.surface = list[0]\n",
        "          self.base = morpheme_info[6]\n",
        "          self.pos = morpheme_info[0]\n",
        "          self.pos1 = morpheme_info[1]"
      ],
