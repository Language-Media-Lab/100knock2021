        "id": "S-UP7dYhVoCp",
        "outputId": "fe491368-ea35-4b91-c2fd-805a10ff77dc"
      },
      "source": [
        "def append_sentence(chunks, sentences):\n",
        "    # 係り元を代入\n",
        "    for i, chunk in enumerate(chunks):\n",
        "      if chunk.dst != -1:\n",
        "          chunks[chunk.dst].srcs.append(i)\n",
        "    sentences.append(chunks)\n",
        "    return sentences\n",
        "\n",
        "\n",
        "\n",
        "data = open('./ai.ja.txt.parsed', 'r') #'r':ファイルの読み込み　'w':書き込み\n",
        "answer41 = []\n",
        "chunks = []\n",
        "onesentence = []\n",
        "for line in data:\n",
        "    if line[0] == '*':  # 係り受け関係を表す行\n",
        "      if len(onesentence) > 0:\n",
        "        chunks.append(Chunk(onesentence, dst))\n",
        "        onesentence = []\n",
        "      Phrase_number = line.split(' ')\n",
        "      dst = int(Phrase_number[2].rstrip('D'))#rstrip関数は、文字列.rstrip(除去する文字)の形で、文字を除去。ここでは、文節番号のDを除去している。\n",
        "    elif line != 'EOS\\n': #一文の終わりには必ず「EOS」がつく。\n",
        "      if Morph(line).pos != '記号':\n",
        "        onesentence.append(Morph(line))\n",
        "    else:  # 形態素リストを答えのリストに追加\n",
        "      chunks.append(Chunk(onesentence,dst))\n",
        "      answer41 = append_sentence(chunks,answer41)\n",
        "      #それぞれを初期化\n",
        "      onesentence = []\n",
        "      chunks = []\n",
        "      dst=-1\n",
        "\n",
        "# 確認\n",
        "for chunk in answer41[2]:\n",
        "  print([morph.surface for morph in chunk.morphs], '係り先:',chunk.dst, '係り元:',chunk.srcs)"
      ],
